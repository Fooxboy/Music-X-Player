<ResourceDictionary
    x:Class="Fooxboy.MusicX.Uwp.DataTemplates.TrackDataTemplateCode"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:Fooxboy.MusicX.Uwp.Resources.Controls"
    xmlns:local="using:Fooxboy.MusicX.Uwp.DataTemplates"
    xmlns:models="using:Fooxboy.MusicX.Core.Models">
    <DataTemplate
        x:Name="TrackDataTemplate"
        x:DataType="models:Audio"
        x:DefaultBindMode="TwoWay">
        <controls:TrackControl HorizontalAlignment="Stretch" Track="{Binding}" />
    </DataTemplate>

</ResourceDictionary>
