    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedTheme="Light"
    xmlns:local="using:Fooxboy.MusicX.Uwp">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/SliderPlayerStyle.xaml"/>
                <ResourceDictionary Source="Resources/Styles/ButtonPlayerStyle.xaml"/>
                <ResourceDictionary Source="Resources/Styles/ListViewStyle.xaml"/>
                <ResourceDictionary Source="Resources/Templates/PlaylistTemplate.xaml"/>
                <ResourceDictionary Source="Resources/Templates/TrackTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
